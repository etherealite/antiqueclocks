{
    "name": "ebangham/antiqueclocks",
    "description": "Antique Clock Price Guide Wordpress",
    "authors": [
        {
            "name"    : "Evan Bangham",
            "homepage": "https://github.com/etherealite"
        }
    ],
    "type": "project",
    "repositories": [
        {
          "type": "composer",
          "url": "https://wpackagist.org",
          "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
        },
        {
            "type": "vcs",
            "url": "https://github.com/etherealite/disable-comments-mu",
            "no-api": true
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^2.0",
        "solarissmoke/disable-comments-mu": "dev-master",
        "wpackagist-theme/generatepress": "^3.1",
        "roots/bedrock-autoloader": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.6.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
          "composer/installers": true
        },
        "vendor-dir": "vendor"
      },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "wordpress/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wordpress/wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
